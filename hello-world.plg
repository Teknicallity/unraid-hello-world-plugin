<?xml version="1.0"?>
<PLUGIN name="hello-world">
  <AUTHOR>Teknicallity</AUTHOR>
  <VERSION>2024.12.27</VERSION>
  <URL>https://github.com/Teknicallity/unraid-hello-world-plugin</URL>
  <DESCRIPTION>Hello World plugin for Unraid</DESCRIPTION>
  <CHANGES>
